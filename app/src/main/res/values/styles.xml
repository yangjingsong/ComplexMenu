<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="ComplexMenuView">
        <attr name="divide_height" format="dimension"></attr>
        <attr name="divide_color" format="color"></attr>
        <attr name="divide_height_vertical" format="dimension"></attr>
        <attr name="divide_width_vertical" format="dimension"></attr>
        <attr name="divide_color_vertical" format="color"></attr>
    </declare-styleable>
</resources>
